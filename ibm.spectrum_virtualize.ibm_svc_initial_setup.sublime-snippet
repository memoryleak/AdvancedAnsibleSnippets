    clustername: ${3:# The hostname or management IP of the Spectrum Virtualize storage system}
    compression: ${4:# Changes system licensing for the compression function}
    dnsip: ${5:# Specifies the DNS server Internet Protocol (IP) address}
    dnsname: ${6:# Specifies a unique name for the system DNS server being created}
