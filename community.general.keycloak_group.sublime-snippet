    auth_password: ${6:# Password to authenticate for API access with}
    auth_realm: ${7:# Keycloak realm name to authenticate to for API access}
    auth_username: ${8:# Username to authenticate for API access with}
    connection_timeout: ${9:# Controls the HTTP connections timeout period (in seconds) to Keycloak API}
    http_agent: ${10:# Configures the HTTP User-Agent header}
    id: ${11:# The unique identifier for this group}
    name: ${12:# Name of the group}
    parents: ${13:# List of parent groups for the group to handle sorted top to bottom}
    realm: ${14:# They Keycloak realm under which this group resides}
    state: ${15:# State of the group}
    token: ${16:# Authentication token for Keycloak API}
    validate_certs: ${17:# Verify TLS certificates (do not disable this in production)}
