  <content><![CDATA[- name: ${1:Create, drop, or alter a PostgreSQL sequence}
  community.postgresql.postgresql_sequence:
    sequence: ${2:# The name of the sequence}
    state: ${3:# The sequence state}
    data_type: ${4:# Specifies the data type of the sequence}
    increment: ${5:# Increment specifies which value is added to the current sequence value to create a new value}
    minvalue: ${6:# Minvalue determines the minimum value a sequence can generate}
    maxvalue: ${7:# Maxvalue determines the maximum value for the sequence}
    start: ${8:# Start allows the sequence to begin anywhere}
    cache: ${9:# Cache specifies how many sequence numbers are to be preallocated and stored in memory for faster access}
    cycle: ${10:# The cycle option allows the sequence to wrap around when the I(maxvalue) or I(minvalue) has been reached by an ascending or descending sequence respectively}
    cascade: ${11:# Automatically drop objects that depend on the sequence, and in turn all objects that depend on those objects}
    rename_to: ${12:# The new name for the I(sequence)}
    owner: ${13:# Set the owner for the I(sequence)}
    schema: ${14:# The schema of the I(sequence)}
    newschema: ${15:# The new schema for the I(sequence)}
    session_role: ${16:# Switch to session_role after connecting}
    db: ${17:# Name of database to connect to and run queries against}
    trust_input: ${18:# If C(false), check whether values of parameters I(sequence), I(schema), I(rename_to), I(owner), I(newschema), I(session_role) are potentially dangerous}
    login_user: ${19:# The username this module should use to establish its PostgreSQL session}
    login_password: ${20:# The password this module should use to establish its PostgreSQL session}
    login_host: ${21:# Host running the database}
    login_unix_socket: ${22:# Path to a Unix domain socket for local connections}
    port: ${23:# Database port to connect to}
    ssl_mode: ${24:# Determines whether or with what priority a secure SSL TCP/IP connection will be negotiated with the server}
    ca_cert: ${25:# Specifies the name of a file containing SSL certificate authority (CA) certificate(s)}
    connect_params: ${26:# Any additional parameters to be passed to libpg}
