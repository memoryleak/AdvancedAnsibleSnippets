    user_accessserial: ${13:# If the user has access to the WTI device via Serial ports}
    user_accessweb: ${14:# If the user has access to the WTI device via Web}
    user_accessapi: ${15:# If the user has access to the WTI device via RESTful APIs}
    user_accessmonitor: ${16:# If the user has ability to monitor connection sessions}
    user_accessoutbound: ${17:# If the user has ability to initiate Outbound connection}
    user_portaccess: ${18:# If AccessLevel is lower than Administrator, which ports the user has access}
    user_plugaccess: ${19:# If AccessLevel is lower than Administrator, which plugs the user has access}
    user_groupaccess: ${20:# If AccessLevel is lower than Administrator, which Groups the user has access}
    user_callbackphone: ${21:# This is the Call Back phone number used for POTS modem connections}
