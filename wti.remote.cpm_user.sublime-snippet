    validate_certs: ${7:# If false, SSL certificates will not be validated}
    use_proxy: ${8:# Flag to control if the lookup will observe HTTP proxy environment variables when present}
    user_name: ${9:# This is the User Name that needs to be create/modified/deleted}
    user_pass: ${10:# This is the User Password that needs to be create/modified/deleted}
    user_accesslevel: ${11:# This is the access level that needs to be create/modified/deleted}
