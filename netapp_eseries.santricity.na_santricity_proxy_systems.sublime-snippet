  netapp_eseries.santricity.na_santricity_proxy_systems:
    add_discovered_systems: ${2:# This flag will force all discovered storage systems to be added to SANtricity Web Services Proxy}
    systems: ${3:# List of storage system information which defines which systems should be added on SANtricity Web Services Proxy}
    subnet_mask: ${4:# This is the IPv4 search range for discovering E-Series storage arrays}
    password: ${5:# Default storage system password which will be used anytime when password has not been provided in the I(systems) sub-options}
    tags: ${6:# Default meta tags to associate with all storage systems if not otherwise specified in I(systems) sub-options}
    accept_certificate: ${7:# Accept the storage system's certificate automatically even when it is self-signed}
    api_username: ${8:# The username to authenticate with the SANtricity Web Services Proxy or Embedded Web Services API}
    api_password: ${9:# The password to authenticate with the SANtricity Web Services Proxy or Embedded Web Services API}
    api_url: ${10:# The url to the SANtricity Web Services Proxy or Embedded Web Services API}
