    mdm_new_name: ${4:# To rename the MDM}
    standby_mdm: ${5:# Specifies add standby MDM parameters}
    is_primary: ${6:# Set I(is_primary) as C(true) to change MDM cluster ownership from the current master MDM to different MDM}
    cluster_mode: ${7:# Mode of the cluster}
    mdm: ${8:# Specifies parameters to add/remove MDMs to/from the MDM cluster}
    mdm_state: ${9:# Mapping state of MDM}
    virtual_ip_interfaces: ${10:# List of interfaces to be used for virtual IPs}
    clear_interfaces: ${11:# Clear all virtual IP interfaces}
    performance_profile: ${12:# Apply performance profile to cluster MDMs}
    state: ${13:# State of the MDM cluster}
    hostname: ${14:# IP or FQDN of the PowerFlex host}
    username: ${15:# The username of the PowerFlex host}
    password: ${16:# The password of the PowerFlex host}
    validate_certs: ${17:# Boolean variable to specify whether or not to validate SSL certificate}
