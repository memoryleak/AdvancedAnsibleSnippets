    generation: ${6:# Specifies the snapshot generation to restore}
    log_path: ${7:# Path of debug log file}
    password: ${8:# REST API password for the Storage Virtualize system}
    restoreuid: ${9:# Specifies the UID of the restored volume should be set to the UID of the volume snapshot that is being restored}
    source_volume_uid: ${10:# Specifies the volume snapshot to restore (specified by volume UID)}
    target_volume_name: ${11:# Specifies the volume name to restore onto}
    token: ${12:# The authentication token to verify a user on the Storage Virtualize system}
    username: ${13:# REST API username for the Storage Virtualize system}
    validate_certs: ${14:# Validates certification}
