    mode: ${10:# Switches the module behaviour between upload, download, get_url (return download url) , get_str (download object as string), create (bucket) and delete (bucket)}
    gs_secret_key: ${11:# GS secret key}
    gs_access_key: ${12:# GS access key}
    region: ${13:# The gs region to use}
    versioning: ${14:# Whether versioning is enabled or disabled (note that once versioning is enabled, it can only be suspended)}
