    allowReplacementOfCertificates: ${6:# Allow Replacement of certificates (required)}
    allowReplacementOfPortalGroupTag: ${7:# Allow Replacement of Portal Group Tag (required)}
    allowRoleTransferForSameSubject: ${8:# Allow transfer of roles for certificate with matching subject}
    allowSHA1Certificates: ${9:# Allow SHA1 based certificates (required)}
    allowWildCardCertificates: ${10:# Allow Wildcard certificates}
    data: ${11:# Certificate Content (required)}
    eap: ${12:# Use certificate for EAP protocols that use SSL/TLS tunneling}
    ims: ${13:# Use certificate for the Cisco ISE Messaging Service}
    name: ${14:# Name of the certificate}
    password: ${15:# Certificate Password (required)}
    portal: ${16:# Use for portal}
    portalGroupTag: ${17:# Set Group tag}
