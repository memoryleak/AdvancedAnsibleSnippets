    ise_version: ${27:# Informs the SDK which version of Identity Services Engine to use}
    ise_wait_on_rate_limit: ${28:# Flag for Identity Services Engine SDK to enable automatic rate-limit handling}
    ise_debug: ${29:# Flag for Identity Services Engine SDK to enable debugging}
    ise_uses_api_gateway: ${30:# Flag that informs the SDK whether to use the Identity Services Engine's API Gateway to send requests}
