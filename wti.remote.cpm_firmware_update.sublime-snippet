  wti.remote.cpm_firmware_update:
    cpm_file: ${2:# If a file is defined, this file will be used to update the WTI device}
    cpm_password: ${3:# This is the Password of the WTI device to send the module}
    cpm_path: ${4:# This is the directory path to store the WTI device configuration file}
    cpm_url: ${5:# This is the URL of the WTI device to send the module}
    cpm_username: ${6:# This is the Username of the WTI device to send the module}
    family: ${7:# Force the download to both either Console (1) or Power (0)}
    removefileonexit: ${8:# After an upgrade, remove the upgrade OS image}
    use_force: ${9:# If set to True, the upgrade will happen even if the device doesnt need it}
    use_https: ${10:# Designates to use an https connection or http connection}
    use_proxy: ${11:# Flag to control if the lookup will observe HTTP proxy environment variables when present}
    validate_certs: ${12:# If false, SSL certificates will not be validated}
