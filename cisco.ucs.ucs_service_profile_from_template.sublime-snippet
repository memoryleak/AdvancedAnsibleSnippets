    state: ${11:# If C(present), will verify Service Profiles are present and will create if needed}
    use_proxy: ${12:# If C(no), will not use the proxy as defined by system environment variable}
    use_ssl: ${13:# If C(no), an HTTP connection will be used instead of the default HTTPS connection}
