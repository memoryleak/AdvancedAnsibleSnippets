    attributes: ${3:# Specifies attributes for files system separated by comma}
    auto_mount: ${4:# File system is automatically mounted at system restart}
    device: ${5:# Logical volume (LV) device name or remote export device to create a NFS file system}
    filesystem: ${6:# Specifies the mount point, which is the directory where the file system will be mounted}
    fs_type: ${7:# Specifies the virtual file system type}
    mount_group: ${8:# Specifies the mount group}
    nfs_server: ${9:# Specifies a Network File System (NFS) server}
    permissions: ${10:# Set file system permissions}
    rm_mount_point: ${11:# Removes the mount point directory when used with state V(absent)}
    size: ${12:# Specifies the file system size}
    state: ${13:# Controls the file system state}
    vg: ${14:# Specifies an existing volume group (VG)}
