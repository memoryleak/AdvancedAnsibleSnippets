  dellemc.unity.cifsserver:
    nas_server_name: ${2:# Name of the NAS server on which CIFS server will be hosted}
    nas_server_id: ${3:# ID of the NAS server on which CIFS server will be hosted}
    netbios_name: ${4:# The computer name of the SMB server in Windows network}
    workgroup: ${5:# Standalone SMB server workgroup}
    local_password: ${6:# Standalone SMB server administrator password}
    domain: ${7:# The domain name where the SMB server is registered in Active Directory}
    domain_username: ${8:# Active Directory domain user name}
    domain_password: ${9:# Active Directory domain password}
    cifs_server_name: ${10:# The name of the CIFS server}
    cifs_server_id: ${11:# The ID of the CIFS server}
    interfaces: ${12:# List of file IP interfaces that service CIFS protocol of SMB server}
    unjoin_cifs_server_account: ${13:# Keep SMB server account unjoined in Active Directory after deletion}
    state: ${14:# Define whether the CIFS server should exist or not}
    unispherehost: ${15:# IP or FQDN of the Unity management server}
    username: ${16:# The username of the Unity management server}
    password: ${17:# The password of the Unity management server}
    validate_certs: ${18:# Boolean variable to specify whether or not to validate SSL certificate}
    port: ${19:# Port number through which communication happens with Unity management server}
